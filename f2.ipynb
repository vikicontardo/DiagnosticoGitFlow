    "def f2_top10users(tweets, users):\n",
    "    tweets_users = pd.merge(tweets, users, on=\"userId\")\n",
    "    tabla = tweets_users.groupby(by=\"username\").count().sort_values(by=\"tweetId\", ascending=False).index.values[:10]\n",
    "    return list(tabla)"
   ]
